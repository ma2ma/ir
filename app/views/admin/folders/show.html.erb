<% provide(:title, @folder.name) %>
<% provide(:page_name, @folder.name) %>
<% provide(:page_summary, @folder.description) %>
<%= render partial: 'shared/show_navbar',locals: {folder: @folder,query_key: @query_key,query_child: @query_child,one_level: @one_level,my_doc: @my_doc} %>
<%= config_tab(0) %>
<% case @folder.folder_type.list_view
  when :thesis %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents,identify_properties: @identify_properties} %>
<% when :book %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents,identify_properties: @identify_properties} %>
<% when :note %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents,properties: @properties,identify_properties: @identify_properties} %>
<% else %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents,identify_properties: @identify_properties} %>
<% end %>
