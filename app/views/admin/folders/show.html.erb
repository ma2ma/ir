<% provide(:title, @folder.name) %>
<% provide(:page_name, @folder.name) %>
<% provide(:page_summary, @folder.description) %>
<%= render partial: 'shared/show_navbar',locals: {folder: @folder} %>
<%= config_tab(0) %>
<% case @folder.folder_type.list_view
  when :thesis %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents} %>
<% when :book %><%= render partial: 'show_thesis',locals: {folder: @folder,document: @document,documents: @documents} %>
<% when :note %><%= render partial: 'show_note',locals: {folder: @folder,document: @document,documents: @documents,properties: @properties} %>
<% end %>
