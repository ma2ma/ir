[
<% @all_users.each_with_index do |user,index| %>
    {
      "query": "<%= user[0].username %> <%= user[0].email %>",
      "isfirend": "true",
      "contact_id": "<%= user[1].to_s %>",
      "name": "<%= user[0].username %>",
      "id": "<%= user[0]._id %>",
      "email": "<%= user[0].email %>",
      "department": "<%= user[0].department.name if user[0].department %>",
      "user_type": "<%= user[0].user_type.name if user[0].user_type %>",
      "avatar_small": "<%= user[0].avatar_url('normal') %>",
      "login_name": "<%= user[0].loginname %>"
    }
<% unless index == @all_users.count - 1%>
    ,
<% end %>
<% end %>
]